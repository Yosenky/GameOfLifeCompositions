// Cell class for grid

public class Cell{
  float cellWidth; // Cell width
  float cellHeight; // Cell height
  float cellX; // Cell X-coordinate
  float cellY; // Cell Y-coordinate
  int lifeState; // If cell is alive(1), if cell is dead(0)
 
  public Cell(float cellWidth, float cellHeight, float cellX, float cellY, int lifeState){
    this.cellWidth = cellWidth;
    this.cellHeight = cellHeight;
    this.cellX = cellX;
    this.cellY = cellY;
    this.lifeState = lifeState;
  }
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
